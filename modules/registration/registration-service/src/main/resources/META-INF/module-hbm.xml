<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-lazy="false">
	<import class="com.liferay.training.amf.registration.model.Registration" />
	<class name="com.liferay.training.amf.registration.model.impl.RegistrationImpl" table="Registration_Registration">
		<id name="userID" type="long">
			<generator class="assigned" />
		</id>
		<property column="uuid_" name="uuid" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="groupId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="firstname" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="lastname" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="email" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="username" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property column="password_" name="password" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="gender" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="dob" type="org.hibernate.type.TimestampType" />
		<property name="homephone" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="mobilephone" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="address1" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="address2" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="city" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property column="state_" name="state" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="zipcode" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="securityquestion" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="answer" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>